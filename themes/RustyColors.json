{
    "$schema": "vscode://schemas/color-theme",
    "semanticHighlighting": true,
    "author": "IoaNN_UwU",
    "name": "Rusty Colors",
    "colors": {
        "activityBar.background": "#333842",
        "activityBar.foreground": "#D7DAE0",
        "activityBarBadge.background": "#528BFF",
        "activityBarBadge.foreground": "#D7DAE0",
        "button.background": "#4D78CC",
        "button.foreground": "#FFFFFF",
        "button.hoverBackground": "#6087CF",
        "diffEditor.insertedTextBackground": "#00809B33",
        "dropdown.background": "#353b45",
        "dropdown.border": "#181A1F",
        "editor.background": "#282C34",
        "editor.lineHighlightBackground": "#2C313A",
        "editor.selectionBackground": "#3E4451",
        "editorCursor.foreground": "#dfdfdf",
        "editorGroup.emptyBackground": "#21252B",
        "editorGroup.border": "#181A1F",
        "editorGroupHeader.tabsBackground": "#21252B",
        "editorIndentGuide.background1": "#3C4049",
        "editorLineNumber.foreground": "#636D83",
        "editorWhitespace.foreground": "#3C4049",
        "editorHoverWidget.background": "#21252B",
        "editorHoverWidget.border": "#181A1F",
        "editorSuggestWidget.background": "#21252B",
        "editorSuggestWidget.border": "#181A1F",
        "editorSuggestWidget.selectedBackground": "#2C313A",
        "editorWidget.background": "#21252B",
        "input.background": "#1B1D23",
        "input.border": "#181A1F",
        "focusBorder": "#9a9a9ab4",
        "list.activeSelectionBackground": "#2C313A",
        "list.activeSelectionForeground": "#D7DAE0",
        "list.focusBackground": "#2C313A",
        "list.hoverBackground": "#2C313A66",
        "list.highlightForeground": "#D7DAE0",
        "list.inactiveSelectionBackground": "#2C313A",
        "list.inactiveSelectionForeground": "#D7DAE0",
        "pickerGroup.border": "#9a9a9ab4",
        "scrollbarSlider.background": "#4E566680",
        "scrollbarSlider.activeBackground": "#747D9180",
        "scrollbarSlider.hoverBackground": "#5A637580",
        "sideBar.background": "#21252B",
        "sideBarSectionHeader.background": "#333842",
        "statusBar.background": "#21252B",
        "statusBar.foreground": "#9DA5B4",
        "statusBarItem.hoverBackground": "#2C313A",
        "statusBar.noFolderBackground": "#21252B",
        "tab.activeBackground": "#282C34",
        "tab.border": "#181A1F",
        "tab.inactiveBackground": "#21252B",
        "titleBar.activeBackground": "#21252B",
        "titleBar.activeForeground": "#9DA5B4",
        "titleBar.inactiveBackground": "#21252B",
        "titleBar.inactiveForeground": "#9DA5B4",
        "statusBar.debuggingForeground": "#FFFFFF",
        "extensionButton.prominentBackground": "#2BA143",
        "extensionButton.prominentHoverBackground": "#37AF4E",
        "badge.background": "#9a9a9ab4",
        "badge.foreground": "#2451a5b4",
        "peekView.border": "#9a9a9ab4",
        "peekViewResult.background": "#21252B",
        "peekViewResult.selectionBackground": "#2C313A",
        "peekViewTitle.background": "#1B1D23",
        "peekViewEditor.background": "#1B1D23"
    },
    "tokenColors": [
        {
            // main text color
            // - local variables
            // - common syntax {},()
            // - function arguments
            "scope": [
                "source",
                "punctuation.brackets.round",
                "keyword.operator.namespace",
                "punctuation.brackets.angle",
                "punctuation.brackets.curly",
                "punctuation.brackets.square",
                "keyword.operator.access",
                "variable",
                "keyword.operator.key-value.rust",
                // C++
                "punctuation.section.block",
                "punctuation.section.parameters",
                "punctuation.section",
                "punctuation.separator",
                "keyword.operator.assignment.cpp",
                "meta.bracket.square",
                "keyword.operator.comparison.cpp",
                "punctuation.definition.capture",
                "punctuation.definition.parameters",
                "keyword.operator.arithmetic.cpp",
                "meta.block.cpp",
                // JS(TS)
                "punctuation.accessor"
            ],
            "settings": {
                "foreground": "#bbb",
            }
        },
        {
            // main keyword color
            // - keywords
            // - important punctuation ;
            // - important operators + - = <
            // - pointers & *
            "scope": [
                "storage.type",
                "keyword.other",
                "keyword.operator.range",
                "keyword.operator.arrow.skinny",
                "keyword.operator.logical",
                "keyword.operator.math",
                "punctuation.semi",
                "punctuation.comma",
                "entity.name.type.numeric",
                "entity.name.type.primitive",
                "keyword.operator.borrow",
                "storage.modifier.mut.rust",
                // string escape characters
                "constant.character.escape",
                "string.quoted.byte.raw.rust",
                "punctuation.section.embedded.rust",
                // C++
                "keyword.control.cpp",
                "variable.language.this.cpp",
                "storage.modifier.reference.cpp",
                "keyword.operator.bitwise.cpp",
                "storage.modifier.specifier",
                "punctuation.terminator",
                "keyword.control",
                "constant.language",
                "storage.modifier",
                "entity.name.function.constructor.cpp",
                // C++ & JS
                "keyword.operator.arithmetic",
                "keyword.operator.bitwise",
                // JS(TS)
                "keyword.operator.",
                "support.type.primitive.ts",
            ],
            "settings": {
                "foreground": "#d9a958"
            }
        },
        {
            // comments
            "scope": [
                "comment",
            ],
            "settings": {
                "foreground": "#5fc26e",
            }
        },
        {
            // strings
            "scope": [
                "string.quoted",
            ],
            "settings": {
                "foreground": "#c2915f",
            }
        },
        {
            // attributes 
            // - [derive(Debug)]
            // - @Service
            "scope": "meta.attribute",
            "settings": {
                "foreground": "#d4d4d4",
            }
        },
        {
            // functions & structs declaration
            // non-primitive types
            "scope": [
                "entity.name.type.struct",
                "entity.name.type.class",
                "entity.name.type.rust",
                // C++
                "entity.name.function.definition.cpp",
                "entity.name.type.parameter.cpp",
                "entity.name.type.cpp",
                "support.type.posix-reserved.c",
                // JS(TS)
                "meta.definition.function",
                "meta.definition.method",
                "entity.name.tag",
                // TOML
                "support.type.property-name.table.toml",
                // MARKDOWN
                "heading",
            ],
            "settings": {
                "foreground": "#de8373",
            }
        },
        {
            // numbers
            "scope": [
                "constant.numeric.decimal",
                // JSON
                "constant.numeric",
            ],
            "settings": {
                "foreground": "#69b6ba"
            }
        },
        {
            // module / namespace
            "scope": [
                "entity.name.namespace",
                // C++
                "string.quoted.other.lt-gt.include.cpp",
                "entity.name.scope-resolution.cpp",
                "entity.name.scope-resolution.function.call.cpp",
                "entity.name.scope-resolution.parameter.cpp",
                "meta.preprocessor.include.cpp",
                // MARKDOWN
                "markup.underline.link.markdown",
                "markup.underline.link.image.markdown",
            ],
            "settings": {
                "foreground": "#70a9b9"
            }
        },
        {
            "scope": [
                "entity.name.type.template.cpp",
            ],
            "settings": {
                "foreground": "#57bdbb",
            }
        },
        {
            // function call
            "scope": [
                "meta.function.call",
                // C++
                "entity.name.function.call",
                "entity.name.function.member.cpp",
                // JS(TS)
                "meta.function-call",
                // MARKDOWN
                "string.other.link.title.markdown",
                "string.other.link.description.markdown",
            ],
            "settings": {
                "foreground": "#7cc2d5",
            }
        },
        {
            "scope": "entity.name.function.preprocessor.cpp",
            "settings": {
                "foreground": "#7b97d2"
            }
        },
        {
            "scope": "entity.name.function.macro.rust",
            "settings": {
                "foreground": "#6797ff"
            }
        },
        {
            "scope": [
                "variable.constant.other",
                "constant.other.caps",
                "variable.other.global.cpp"
            ],
            "settings": {
                "foreground": "#7b97d2"
            }
        },
        {
            // fields of structure
            "scope": [
                // JS(TS)
                "variable.other.property",
                "support.variable.property",
                "entity.other.attribute-name",
                // JSON
                "support.type.property-name.json",
                // TOML
                "support.type.property-name.toml",
                // MARKDOWN
                "markup.inline.raw.string.markdown",
            ],
            "settings": {
                "foreground": "#95acbd"
            }
        },
        {
            "scope": [
                // JS(TS)
                "variable.language.this.tsx",
            ],
            "settings": {
                "foreground": "#91a4d0"
            }
        },
        {
            "scope": [
                "markup.bold.markdown",
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "markup.italic.markdown",
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
    ],
    "semanticTokenColors": {
        "keyword.unsafe": "#E06C75",
        "variable.constant": "#7b97d2",
        "builtinType": "#d9a958",
        "interface": "#82c67c",
        "enum": "#6ec0d9",
        "enumMember": "#91a4d0",
        "method.static": "#e08b83",
        "property": "#95acbd",
        "lifetime": "#798fde",
        "function.async": "#b898cf",
        "keyword.async": "#bb85e1",
        "typeParameter": "#57bdbb",
        "typeAlias": "#de8373",
        "selfKeyword": "#91a4d0",
        "selfTypeKeyword": "#91a4d0",
        "regexp": {
            "color": "#D19A66",
            "fontStyle": "bold"
        },
        "tomlTableKey": "#95acbd",
        "tomlArrayKey": "#95acbd",
        "*.mutable": {
            "fontStyle": "underline"
        },
        "function.injected": "#74c9e0",
        "struct.injected": "#de8373",
        "namespace.injected": "#70a9b9",
        "macro.injected": "#6797ff",
        "string.injected": "#c2915f",
        "keyword.injected": "#d9a958",
        "comment.injected": "#5fc26e",
        "enumMember.injected": "#91a4d0",
        "enum.injected": "#6ec0d9",
        "typeAlias.injected": "#6ec0d9",
        "*.injected": "#bbb",
    }
}