{
    "$schema": "vscode://schemas/color-theme",
    "semanticHighlighting": true,
    "author": "IoaNN_UwU",
    "name": "Rusty Colors Light",
    "colors": {
        "activityBar.background": "#FAFAFA",
        "activityBar.foreground": "#121417",
        "editorInlayHint.background": "#F5F5F5",
        "editorInlayHint.foreground": "#AFB2BB",
        "notebook.cellEditorBackground": "#F5F5F5",
        "activityBarBadge.background": "#526FFF",
        "activityBarBadge.foreground": "#FFFFFF",
        "button.background": "#5871EF",
        "button.foreground": "#FFFFFF",
        "button.hoverBackground": "#6B83ED",
        "diffEditor.insertedTextBackground": "#00809B33",
        "dropdown.background": "#FFFFFF",
        "dropdown.border": "#DBDBDC",
        "editorIndentGuide.activeBackground": "#626772",
        "editor.background": "#FAFAFA",
        "editor.foreground": "#383A42",
        "editor.lineHighlightBackground": "#383A420C",
        "editor.selectionBackground": "#E5E5E6",
        "editorCursor.foreground": "#526FFF",
        "editor.findMatchHighlightBackground": "#526FFF33",
        "editorGroup.background": "#EAEAEB",
        "editorGroup.border": "#DBDBDC",
        "editorGroupHeader.tabsBackground": "#EAEAEB",
        "editorIndentGuide.background": "#383A4233",
        "editorLineNumber.foreground": "#9D9D9F",
        "editorLineNumber.activeForeground": "#383A42",
        "editorWhitespace.foreground": "#383A4233",
        "editorRuler.foreground": "#383A4233",
        "editorHoverWidget.background": "#EAEAEB",
        "editorHoverWidget.border": "#DBDBDC",
        "editorSuggestWidget.background": "#EAEAEB",
        "editorSuggestWidget.border": "#DBDBDC",
        "editorSuggestWidget.selectedBackground": "#FFFFFF",
        "editorWidget.background": "#EAEAEB",
        "editorWidget.border": "#E5E5E6",
        "input.background": "#FFFFFF",
        "input.border": "#DBDBDC",
        "focusBorder": "#526fff16",
        "list.activeSelectionBackground": "#DBDBDC",
        "list.activeSelectionForeground": "#232324",
        "list.focusBackground": "#DBDBDC",
        "list.hoverBackground": "#DBDBDC66",
        "list.highlightForeground": "#121417",
        "list.inactiveSelectionBackground": "#DBDBDC",
        "list.inactiveSelectionForeground": "#232324",
        "notification.background": "#333333",
        "pickerGroup.border": "#526FFF",
        "scrollbarSlider.background": "#4E566680",
        "scrollbarSlider.activeBackground": "#747D9180",
        "scrollbarSlider.hoverBackground": "#5A637580",
        "sideBar.background": "#EAEAEB",
        "sideBarSectionHeader.background": "#FAFAFA",
        "statusBar.background": "#EAEAEB",
        "statusBar.foreground": "#424243",
        "statusBarItem.hoverBackground": "#DBDBDC",
        "statusBar.noFolderBackground": "#EAEAEB",
        "tab.activeBackground": "#FAFAFA",
        "tab.activeForeground": "#121417",
        "tab.border": "#DBDBDC",
        "tab.inactiveBackground": "#EAEAEB",
        "titleBar.activeBackground": "#EAEAEB",
        "titleBar.activeForeground": "#424243",
        "titleBar.inactiveBackground": "#EAEAEB",
        "titleBar.inactiveForeground": "#424243",
        "statusBar.debuggingForeground": "#FFFFFF",
        "extensionButton.prominentBackground": "#3BBA54",
        "extensionButton.prominentHoverBackground": "#4CC263",
        "badge.background": "#526FFF",
        "badge.foreground": "#FFFFFF",
        "peekView.border": "#526FFF",
        "peekViewResult.background": "#EAEAEB",
        "peekViewResult.selectionBackground": "#DBDBDC",
        "peekViewTitle.background": "#FFFFFF",
        "peekViewEditor.background": "#FFFFFF",
        "editorInlayHint.typeForeground": "#9f9f9f",
        "editorInlayHint.typeBackground": "#ebebeb",
        "editorInlayHint.parameterForeground": "#9f9f9f",
        "editorInlayHint.parameterBackground": "#ebebeb",
    },
    "tokenColors": [
        {
            "scope": [
                "source",
                "punctuation.brackets.round",
                "keyword.operator.namespace",
                "punctuation.brackets.angle",
                "punctuation.brackets.curly",
                "punctuation.brackets.square",
                "keyword.operator.access",
                "variable",
                "keyword.operator.key-value.rust",
                // C++
                "punctuation.section.block",
                "punctuation.section.parameters",
                "punctuation.section",
                "punctuation.separator",
                "keyword.operator.assignment.cpp",
                "meta.bracket.square",
                "keyword.operator.comparison.cpp",
                "punctuation.definition.capture",
                "punctuation.definition.parameters",
                "keyword.operator.arithmetic.cpp",
                "meta.block.cpp",
                // JS(TS)
                "punctuation.accessor"
            ],
            "settings": {
                "foreground": "#bbb",
            }
        },
        {
            "scope": [
                "storage.type",
                "keyword.other",
                "keyword.operator.range",
                "keyword.operator.arrow.skinny",
                "keyword.operator.logical",
                "keyword.operator.math",
                "punctuation.semi",
                "punctuation.comma",
                "entity.name.type.numeric",
                "entity.name.type.primitive",
                "keyword.operator.borrow",
                "storage.modifier.mut.rust",
                // C++
                "keyword.control.cpp",
                "storage.modifier.reference.cpp",
                "keyword.operator.bitwise.cpp",
                "storage.modifier.specifier",
                "punctuation.terminator",
                "keyword.control",
                "constant.language",
                // C++ & JS
                "keyword.operator.arithmetic",
                "keyword.operator.bitwise",
                // JS(TS)
                "keyword.operator.",
                "support.type.primitive.ts",
            ],
            "settings": {
                "foreground": "#d9a958"
            }
        },
        {
            "scope": [
                "comment",
            ],
            "settings": {
                "foreground": "#5fc26e",
            }
        },
        {
            "scope": [
                "string.quoted",
            ],
            "settings": {
                "foreground": "#c2915f",
            }
        },
        {
            "scope": "meta.attribute",
            "settings": {
                "foreground": "#d4d4d4",
            }
        },
        {
            "scope": [
                "entity.name.type.struct",
                "entity.name.type.class",
                "entity.name.type.rust",
                "meta.function.definition",
                // C++
                "entity.name.function.definition.cpp",
                // JS(TS)
                "meta.definition.function",
                "meta.definition.method",
                "entity.name.tag",
                // TOML
                "support.type.property-name.table.toml"
            ],
            "settings": {
                "foreground": "#de8373",
            }
        },
        {
            "scope": [
                "constant.numeric.decimal",
                // JSON
                "constant.numeric",
            ],
            "settings": {
                "foreground": "#69b6ba"
            }
        },
        {
            "scope": "entity.name.namespace.cpp",
            "settings": {
                "foreground": "#ba7d69"
            }
        },
        {
            "scope": [
                "entity.name.namespace",
                // C++
                "string.quoted.other.lt-gt.include.cpp",
                "meta.preprocessor.include.cpp",
            ],
            "settings": {
                "foreground": "#70a9b9"
            }
        },
        {
            "scope": [
                "entity.name.type.template.cpp",
            ],
            "settings": {
                "foreground": "#57bdbb",
            }
        },
        {
            "scope": [
                "meta.function.call",
                // C++
                "entity.name.function.call",
                "entity.name.function.member.cpp",
                // JS(TS)
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#74c9e0",
            }
        },
        {
            "scope": "entity.name.function.preprocessor.cpp",
            "settings": {
                "foreground": "#7b97d2"
            }
        },
        {
            "scope": "entity.name.function.macro",
            "settings": {
                "foreground": "#6797ff"
            }
        },
        {
            "scope": [
                "variable.constant.other",
                "constant.other.caps",
                "variable.other.global.cpp"
            ],
            "settings": {
                "foreground": "#7b97d2"
            }
        },
        {
            "scope": [
                // JS(TS)
                "variable.other.property",
                "support.variable.property",
                "entity.other.attribute-name",
                // JSON
                "support.type.property-name.json",
                // TOML
                "support.type.property-name.toml"
            ],
            "settings": {
                "foreground": "#95acbd"
            }
        },
        {
            "scope": [
                // JS(TS)
                "variable.language.this.tsx",
            ],
            "settings": {
                "foreground": "#91a4d0"
            }
        },
    ],
    "semanticTokenColors": {
        "formatSpecifier": "#e2d49c",
        "keyword.unsafe": "#E06C75",
        "variable.constant": "#7b97d2",
        "builtinType": "#d9a958",
        "interface": "#82c67c",
        "enum": "#6ec0d9",
        "enumMember": "#91a4d0",
        "method.static": "#e08b83",
        "property": "#95acbd",
        "lifetime": "#798fde",
        "function.async": "#b898cf",
        "keyword.async": "#bb85e1",
        "typeParameter": "#57bdbb",
        "typeAlias": "#de8373",
        "selfKeyword": "#91a4d0",
        "selfTypeKeyword": "#91a4d0",
        "regexp": {
            "color": "#D19A66",
            "fontStyle": "bold"
        },
        "tomlTableKey": "#95acbd",
        "tomlArrayKey": "#95acbd",
        "*.mutable": {
            "fontStyle": "underline"
        },
        "function.injected": "#74c9e0",
        "struct.injected": "#de8373",
        "namespace.injected": "#70a9b9",
        "macro.injected": "#6797ff",
        "string.injected": "#c2915f",
        "keyword.injected": "#d9a958",
        "comment.injected": "#5fc26e",
        "enumMember.injected": "#91a4d0",
        "enum.injected": "#6ec0d9",
        "typeAlias.injected": "#6ec0d9",
        "*.injected": "#bbb"
    }
}